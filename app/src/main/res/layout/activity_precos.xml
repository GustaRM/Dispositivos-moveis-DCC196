<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#676565"
    tools:context=".Precos">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        tools:context=".Menu_cliente"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-25dp">

        <TextView
            android:id="@+id/txtTitulo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:background="#000000"
            android:fontFamily="@font/archivo_black"
            android:text="PreÃ§os"
            android:textColor="#FFFFFF"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="30dp"
            android:rotation="-170"
            android:src="@drawable/pngwing_com"
            app:layout_constraintEnd_toStartOf="@+id/txtTitulo"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="52dp"
            android:rotation="-174"
            android:rotationY="170"
            android:src="@drawable/pngwing_com"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/txtTitulo"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btnSair"
            android:layout_width="125dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="48dp"
            android:fontFamily="@font/archivo_black"
            android:text="Voltar"
            android:textSize="16sp"
            app:backgroundTint="#3351BC"
            app:cornerRadius="10px"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.486"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/listaAgendamentos"
            app:layout_constraintVertical_bias="0.642" />

        <ListView
            android:id="@+id/listaAgendamentos"
            android:layout_width="271dp"
            android:layout_height="284dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="68dp"
            android:layout_marginEnd="36dp"
            android:background="#7A7878"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtTitulo" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>